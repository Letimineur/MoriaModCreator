<?xml version='1.0' encoding='UTF-8'?>
<definition>
  <title>Properties_RedBeryl DropRate Fix</title>
  <author>kenuaena</author>
  <description>Adds DropRate parameter to Properties_RedBeryl</description>
  <mod file="Moria\Content\Environments\Voxels\Baking\Properties_RedBeryl.json">
    <change item="Properties_RedBeryl" property="PrimaryDrop.DropRate" value="1800.0">
      <add_property item="Properties_RedBeryl"><![CDATA[{
        "$type": "UAssetAPI.PropertyTypes.Objects.FloatPropertyData, UAssetAPI",
        "Name": "DropRate",
        "Value": 0.0
      }]]></add_property>
    </change>
  </mod>
</definition>
